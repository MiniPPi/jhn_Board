<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="boardMapper">

<select id="boardList" resultType="com.kh.app.vo.ListVo">
      
 	  SELECT NO,TITLE,ENROLLDATE,ID AS WRITER FROM LIST ORDER BY NO DESC  
       
      
  </select>
  
  <select id="selectCount" resultType="int">
      SELECT COUNT(*) FROM LIST
  </select>
  
  <insert id="boardWrite">
          INSERT INTO FAQ (NO,ID,TITLE,CONTENT,ENROLL_DATE) 
        VALUES(SEQ_LIST_NO.NEXTVAL,#{writer},#{title},
        #{content}, SYSDATE)
  </insert>
</mapper>
